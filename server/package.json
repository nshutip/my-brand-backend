{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha test/**/*.js",
    "test-with-coverage": "nyc report --reporter=text",
    "coverage": "nyc npm run test",
    "start": "node server.js"
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "check-coverage": true,
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/dist/**",
      "**/swagger/**"
    ],
    "reporter": [
      "text",
      "lcov",
      "html"
    ],
    "report-dir": "./coverage",
    "all": true,
    "excludeNodeModules": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "istanbul": "^0.4.5",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.0",
    "multer": "^1.4.5-lts.1",
    "should": "^13.2.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.1",
    "istanbul-lib-coverage": "^3.2.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0"
  },
  "description": ""
}
